  "homepage": "https://github.com/solblack/xml2customjs",
  "dependencies": {
    "xmlbuilder2": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
